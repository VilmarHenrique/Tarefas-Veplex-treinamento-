<?php
system("clear");

$arr = array(
    array("Fernando", 34),
    array("Marcelo", 25),
    array("Roberto", 30),
    array("Sandra", 22),
    array("Tiago", 18)
    );

    for($i = 0; $i < count($arr); $i++){
        if($arr[$i][1] <= 25){
            echo '\n'; 
            print_r($arr[$i]); 
            echo '\n';
        
        }
    }
