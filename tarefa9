<?php

$n = readline("número: ");
$primos = [];


function verificaSeÉPrimo($n){

for($count=2; $count<$n; $count++){
 if($n % $count == 0){
  return false;
 }}
 return true;
}

for($i = 0; $i < $n; $i++){

if(verificaSeÉPrimo($i)){
    array_push($primos, $i);

}    

 }
 echo '<pre>'; print_r($primos); 
